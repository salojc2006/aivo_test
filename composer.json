{
    "name": "salojc2006/aivo_test",
    "description": "Test example for aivo.",
    "keywords": [
		"spotify",
		"php7",
		"slim",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "https://github.com/salojc2006/aivo_test",    
    "authors": [
        {
            "name": "Salomon Cordova",
            "email": "salojc2006@gmail.com",
            "homepage": "https://github.com/salojc2006/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.0",
        "monolog/monolog": "^1.24",
        "php-di/php-di": "^6.0",
        "slim/psr7": "^0.5",
        "slim/slim": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
